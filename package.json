{
  "version": "13.0.5",
  "name": "@cantoo/rn-svg",
  "description": "SVG library for react-native",
  "homepage": "https://github.com/cantoo-scribe/rn-svg",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cantoo-scribe/rn-svg.git"
  },
  "license": "MIT",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.ts",
  "types": "index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "__tests__",
    "android",
    "apple",
    "elements",
    "RNSVG.podspec",
    "index.d.ts",
    "index.js.flow"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "keywords": [
    "react-component",
    "react-native",
    "ios",
    "android",
    "SVG",
    "ART",
    "VML",
    "gradient"
  ],
  "scripts": {
    "bob": "bob build",
    "flow": "flow src",
    "flowgen": "flowgen index.d.ts -o index.js.flow",
    "flowtyped": "flow-typed install",
    "format": "prettier README.md CONTRIBUTING.md CODE_OF_CONDUCT.md './src/**/*.{ts,tsx}' index.d.ts --write",
    "jest": "jest",
    "lint": "eslint --ext .ts,.tsx src",
    "peg": "pegjs -o src/lib/extract/transform.js ./src/lib/extract/transform.peg",
    "prepare": "npm run flowgen && npm run bob",
    "test": "npm run lint && npm run tsc && npm run flow && npm run jest",
    "tsc": "tsc --noEmit",
    "release": "npm run prepare && release-it"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": ">=0.50.0",
    "react-native-web": ">=0.14.0"
  },
  "dependencies": {
    "css-select": "^2.1.0",
    "css-tree": "^1.0.0-alpha.39"
  },
  "devDependencies": {
    "@babel/compat-data": ">=7.17.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/eslint-plugin": "^1.3.0",
    "@types/css-tree": "^1.0.3",
    "@types/react": "^17.0.43",
    "@types/react-native": "^0.67.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "eslint": "^8.12.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "flow-bin": "^0.175.0",
    "flow-typed": "^3.7.0",
    "flowgen": "^1.17.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": ">=0.66.0",
    "pegjs": "^0.10.0",
    "prettier": "^2.6.2",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-native": ">=0.68.0",
    "react-native-builder-bob": ">=0.18.2",
    "react-native-web": ">=0.17.7",
    "react-test-renderer": ">=17.0.2",
    "release-it": "^13.6.8",
    "typescript": ">=4.6.3"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "nativePackage": true,
  "optionalDependencies": {
    "react-native-web": ">=0.14.0"
  },
  "bugs": {
    "url": "https://github.com/cantoo-scribe/rn-svg/issues"
  },
  "directories": {
    "lib": "lib"
  },
  "author": "Cantoo",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
